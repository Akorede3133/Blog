<%= form_with model: [current_user, current_user.posts.build], url: user_posts_path(current_user) do |f| %>
  <div>
    <%= f.label :title %><br>
    <%= f.text_field :title%>
  </div>
  <div>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>